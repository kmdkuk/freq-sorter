{
  "manifest_version": 3,
  "name": "FreqSorter",
  "version": "0.1.0",
  "description": "A Chrome extension that automatically sorts your bookmarks based on usage frequency.",
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "background": {
    "service_worker": "src/background/index.ts",
    "type": "module"
  },
  "permissions": ["bookmarks", "tabs", "storage", "alarms"]
}
