{
  "manifest_version": 3,
  "name": "Bookmark Sorter",
  "version": "1.0.0",
  "description": "Sort bookmarks by usage frequency.",
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "background": {
    "service_worker": "src/background/index.ts",
    "type": "module"
  },
  "permissions": ["bookmarks", "tabs", "storage", "alarms"]
}
